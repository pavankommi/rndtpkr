{
  "name": "rndtpkr",
  "version": "4.0.3",
  "description": "Lightweight React Native date/time picker with auto system format, looping wheels, and minute stepping",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "lib/module/index",
  "source": "src/index",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "react-native": "./lib/module/index.js",
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js",
      "default": "./lib/commonjs/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "npx tsc --noEmit",
    "lint": "eslint .",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "react-native-datetime",
    "react-native-datetime-picker",
    "react-native-datepicker",
    "react-native-timepicker",
    "react-native-time-picker",
    "react-native-calendar",
    "react-native-daterange",
    "datetime",
    "datetime-picker",
    "datepicker",
    "timepicker",
    "time-picker",
    "calendar",
    "daterange",
    "wheel-picker",
    "date-range"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavankommi/rndtpkr.git"
  },
  "author": "Pavan Kommi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pavankommi/rndtpkr/issues"
  },
  "homepage": "https://github.com/pavankommi/rndtpkr#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.0.0",
    "@evilmartians/lefthook": "^1.13.6",
    "@react-native-community/eslint-config": "^3.0.2",
    "@react-native/eslint-config": "^0.77.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.3.2",
    "@types/jest": "^30.0.0",
    "@types/react": "18.2.0",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^8.45.0",
    "commitlint": "^20.1.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.2.0",
    "pod-install": "^0.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-builder-bob": "^0.20.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.3"
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-native": "^0.72.8"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.64.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-native": {
      "optional": false
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "npm@10.8.2",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "trailingComma": "es5",
          "tabWidth": 2,
          "semi": true,
          "singleQuote": true,
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "example/",
    "demo/",
    "*.config.js",
    "tsconfig.json"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "dequal": "^2.0.3",
    "react-native-localize": "^3.5.2",
    "tailwind-merge": "^3.3.1"
  }
}
